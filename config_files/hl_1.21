     -2y '^[ 	]+-%?2?y[ 	]+(.*)'
     -2b '^[ 	]+-%?2?b[ 	]+(.*)'
     -2m '^[ 	]+-%?2?m[ 	]+(.*)'
     -2c '^[ 	]+-%?2?c[ 	]+(.*)'
     -2w '^[ 	]+-%?2?w[ 	]+(.*)'
