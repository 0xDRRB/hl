     -3c '^[ 	]+-%?3c[ 	]+(.*)'
     -3w '^[ 	]+-%?3w[ 	]+(.*)'
     -1R '^[ 	]+-%?1R[ 	]+(.*)'
     -1G '^[ 	]+-%?1G[ 	]+(.*)'
     -1Y '^[ 	]+-%?1Y[ 	]+(.*)'
     -1B '^[ 	]+-%?1B[ 	]+(.*)'
     -1M '^[ 	]+-%?1M[ 	]+(.*)'
     -1C '^[ 	]+-%?1C[ 	]+(.*)'
     -1W '^[ 	]+-%?1W[ 	]+(.*)'
     -2R '^[ 	]+-%?2?R[ 	]+(.*)'
     -2G '^[ 	]+-%?2?G[ 	]+(.*)'
     -2Y '^[ 	]+-%?2?Y[ 	]+(.*)'
